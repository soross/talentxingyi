<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:imo="http://schemas.android.com/apk/res/com.imo"
    style="@style/background_default"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="10dp"
    android:paddingRight="10dp"
     >
     
	<include layout="@layout/vertical_space"/>
    
    <com.imo.view.SettingItemView
        android:id="@+id/settingItemView1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        imo:leftsrc="@drawable/icon_edit_work_sign" 
        imo:itemText="@string/app_name"
        imo:rightsrc="@drawable/triangle_icon" 
        >
    </com.imo.view.SettingItemView>
    
  <include layout="@layout/vertical_space"/> 
    
    <com.imo.view.SettingItemView
        android:id="@+id/settingItemView2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" 
        imo:leftsrc="@drawable/icon_edit_work_sign" 
        imo:itemText="@string/app_name"
        imo:rightsrc="@drawable/triangle_icon" 
        >
    </com.imo.view.SettingItemView>
    
    <com.imo.view.SettingItemView
        android:id="@+id/settingItemView3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" 
        imo:leftsrc="@drawable/icon_edit_work_sign" 
        imo:itemText="@string/app_name"
        imo:rightsrc="@drawable/triangle_icon" 
        android:layout_marginTop="20dip"
        >
    </com.imo.view.SettingItemView>

</LinearLayout>